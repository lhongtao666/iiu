<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>t9-ecm</artifactId>
	<packaging>war</packaging>

	<parent>
		<groupId>com.jt.ecm</groupId>
		<artifactId>t9-ecm-root</artifactId>
		<version>1.3.0-SNAPSHOT</version>
		<relativePath>../../pom.xml</relativePath>
	</parent>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<log4j.version>1.2.16</log4j.version>
		<dubbo.provider.port>20888</dubbo.provider.port>
		<run.properties.machine.id>1</run.properties.machine.id>
		<websocket.ip>119.23.111.209:8055</websocket.ip>
		<db_jtsoft_ecm>test_jtsoft_ecm</db_jtsoft_ecm>
		<db_jtsoft_csfollows>test_jtsoft_csfollows</db_jtsoft_csfollows>
		<db_jtsoft_log>test_jtsoft_logs</db_jtsoft_log>
		<db_jtsoft_weixin>test_jtsoft_weixin_records</db_jtsoft_weixin>
		<uat.time>${maven.build.timestamp}</uat.time>
	</properties>

	<dependencies>
		<dependency>
			<groupId>com.jt.base</groupId>
			<artifactId>jt-base-web</artifactId>
		</dependency>
		<dependency>
			<groupId>com.jt.credit</groupId>
			<artifactId>jt-credit-core</artifactId>
		</dependency>
		<dependency>
			<groupId>com.jt.ecm</groupId>
			<artifactId>t9-ecm-biz-ext</artifactId>
		</dependency>
		<dependency>
			<groupId>com.jt.ecm</groupId>
			<artifactId>t9-ecm-biz-transfer</artifactId>
		</dependency>
		<dependency>
			<groupId>com.jt.ecm</groupId>
			<artifactId>t9-ecm-biz-cti</artifactId>
		</dependency>
		<dependency>
			<groupId>log4j</groupId>
			<artifactId>log4j</artifactId>
			<version>${log4j.version}</version>
		</dependency>
		<dependency>
			<groupId>javax.servlet</groupId>
			<artifactId>servlet-api</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.tomcat</groupId>
			<artifactId>tomcat-websocket-api</artifactId>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.bull.javamelody</groupId>
			<artifactId>javamelody-core</artifactId>
		</dependency>

		<!-- dubbo 需要的jar start -->
		<dependency>
			<groupId>org.jboss.netty</groupId>
			<artifactId>netty</artifactId>
		</dependency>
		<dependency>
			<groupId>com.alibaba</groupId>
			<artifactId>dubbo</artifactId>
		</dependency>
		<dependency>
			<groupId>org.apache.zookeeper</groupId>
			<artifactId>zookeeper</artifactId>
		</dependency>
		<dependency>
			<groupId>com.101tec</groupId>
			<artifactId>zkclient</artifactId>
		</dependency>
		<!-- dubbo 需要的jar end -->

	</dependencies>

	<build>

		<finalName>t9-ecm</finalName>
		<resources>
			<resource>
				<directory>${project.basedir}/src/main/resources</directory>
				<filtering>true</filtering>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<profiles>
		<profile>
			<id>uat</id>
			<distributionManagement>
				<repository>
					<id>maven-releases</id>
					<name>maven-releases</name>
					<url>http://192.168.0.153:8081/repository/maven-releases/</url>
				</repository>
				<snapshotRepository>
					<id>maven-snapshots</id>
					<name>maven-snapshots</name>
					<url>http://192.168.0.153:8081/repository/maven-snapshots/</url>
				</snapshotRepository>
			</distributionManagement>
		</profile>
		<profile>
			<id>test-8088</id>
			<properties>
				<dubbo.provider.port>20888</dubbo.provider.port>
				<run.properties.machine.id>1</run.properties.machine.id>
			</properties>
		</profile>
		<profile>
			<id>test-7088</id>
			<properties>
				<dubbo.provider.port>20886</dubbo.provider.port>
				<run.properties.machine.id>2</run.properties.machine.id>
			</properties>
		</profile>
		<profile>
			<id>153</id>
			<properties>
				<websocket.ip>192.168.0.153:8055</websocket.ip>
				<db_jtsoft_ecm>jtsoft_ecm</db_jtsoft_ecm>
				<db_jtsoft_csfollows>jtsoft_csfollows</db_jtsoft_csfollows>
				<db_jtsoft_log>jtsoft_logs</db_jtsoft_log>
				<db_jtsoft_weixin>jtsoft_weixin_records</db_jtsoft_weixin>
			</properties>
		</profile>
	</profiles>
</project>